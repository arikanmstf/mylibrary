{
  "name": "mylibrary",
  "version": "2.2.1",
  "description": "MyLibrary is an online book library that you can manage and share your books.",
  "main": "index.js",
  "repository": "git@github.com:arikanmstf/mylibrary.git",
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "toolbox": "react-toolbox-themr --config src/config/theme/default.json --javascript src/assets/theme --styles assets/theme",
    "lint": "eslint --max-warnings 0 --ext .jsx,.js src/scripts ",
    "prepare": "npm run lint && npm run clean && npm run toolbox",
    "prod": "npm run prepare && webpack-dev-server --env=prod",
    "dev": "npm run prepare && webpack-dev-server --env=dev",
    "dist:prod": "npm run prepare && webpack -p --env=prod",
    "dist:dev": "npm run prepare && webpack --env=dev"
  },
  "author": "Mustafa Arikan",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "node-sass": "^4.5.3",
    "react-router-dom": "^4.1.1",
    "react-toolbox-themr": "^1.0.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1",
    "yargs": "^8.0.1"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-preset-stage-1": "^6.1.18",
    "prop-types": "^15.5.10",
    "qs": "^6.4.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-dropzone": "^3.13.3",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
