{
  "name": "mylibrary",
  "version": "3.0.0-alpha.2",
  "description": "MyLibrary is an online book library that you can manage and share your books.",
  "main": "src/app/native/index.js",
  "repository": "git@github.com:arikanmstf/mylibrary.git",
  "scripts": {
    "start": "yarn run start-web",
    "start-web": "yarn run start-web:local-dev",
    "start-web:local-dev": "webpack-dev-server --config webpack.config.local-dev.js --mode development",
    "start-web:local-prod": "webpack-dev-server --config webpack.config.local-prod.js --mode production",
    "start-web:live-dev": "webpack-dev-server --config webpack.config.live-dev.js --mode development",
    "start-web:live-prod": "webpack-dev-server --config webpack.config.live-prod.js --mode production",
    "build-web:local-dev": "yarn check-src && webpack --config webpack.config.local-dev.js --mode development",
    "build-web:local-prod": "yarn check-src && webpack --config webpack.config.local-prod.js --mode production",
    "build-web:live-prod": "webpack --config webpack.config.live-prod.js --mode production",
    "deploy-web:local": "(docker container stop mylibrary-container & docker container rm mylibrary-container | docker rmi mylibrary-image | yarn build-web:local-prod) && docker build -t mylibrary-image . && docker run -p 8082:80 -d --name mylibrary-container mylibrary-image",
    "start-native": "yarn run start-native:local-dev",
    "start-native:local-dev": "react-native-scripts start",
    "build-ios:local-dev": "expo build:ios",
    "build-ios:live-prod": "expo build:ios",
    "test": "jest",
    "lint": "eslint --max-warnings 0 --ext .js src/",
    "flow": "flow",
    "flow-update": "flow-typed install",
    "fresh-install": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro* && rm -rf node_modules && yarn cache clean && yarn install",
    "check-src": "yarn run test && yarn run lint",
    "create-module": "node src/scripts/create-module/index"
  },
  "keywords": [
    "react",
    "native",
    "template",
    "webpack",
    "redux"
  ],
  "author": "Mustafa Arikan",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.0",
    "eslint-plugin-react": "7.10.0",
    "file-loader": "1.1.11",
    "file-system": "2.2.2",
    "flow-bin": "0.74.0",
    "flow-typed": "2.5.1",
    "jest": "23.3.0",
    "metro-react-native-babel-preset": "0.51.1",
    "path": "0.12.7",
    "prompt": "1.0.0",
    "react-native-scripts": "1.14.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "4.15.1",
    "webpack-cli": "3.0.8",
    "webpack-config-arikanmstf": "1.0.2",
    "webpack-dev-server": "3.1.4",
    "yargs": "11.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@babel/runtime": "7.2.0",
    "@material-ui/core": "3.6.2",
    "@material-ui/icons": "3.0.1",
    "axios": "0.18.0",
    "expo": "31.0.6",
    "immutable": "3.8.2",
    "lodash.debounce": "4.0.8",
    "native-base": "2.10.0",
    "react": "16.6.3",
    "react-dom": "16.4.1",
    "react-native": "0.57.8",
    "react-native-loading-spinner-overlay": "1.0.1",
    "react-native-storage": "0.2.2",
    "react-native-vector-icons": "6.0.2",
    "react-navigation": "2.14.2",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-actions": "2.4.0",
    "redux-form": "7.4.2",
    "redux-immutablejs": "0.0.8",
    "redux-thunk": "2.3.0",
    "styled-components": "3.3.3"
  },
  "jest": {
    "setupFiles": [
      "./test/enzyme-setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base-shoutem-theme|react-router-native)"
    ],
    "preset": "react-native",
    "globals": {
      "IS_DEVELOPMENT": false,
      "IS_PRODUCTION": true,
      "IS_LOCAL": false,
      "IS_LIVE": true,
      "ENVIRONMENT": "live",
      "IS_MOCK": true,
      "window": {
        "navigator": {
          "userAgent": "Mozilla/5.0 (iPhone; CPU iPhone OS 11_0 like Mac OS X) AppleWebKit/604.1.38 (KHTML, like Gecko) Version/11.0 Mobile/15A372 Safari/604.1"
        }
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src/app"
    ]
  }
}
