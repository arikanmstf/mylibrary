{
  "name": "mylibrary",
  "version": "3.0.0",
  "description": "MyLibrary is an online book library that you can manage and share your books.",
  "main": "src/app/native/index.js",
  "repository": "git@github.com:arikanmstf/mylibrary.git",
  "scripts": {
    "start": "yarn run start-web",
    "start-web": "webpack-dev-server --config webpack.config.dev.js --mode development",
    "start-native": "react-native-scripts start",
    "test": "jest",
    "lint": "eslint --max-warnings 0 --ext .js src/",
    "flow": "flow",
    "flow-update": "flow-typed install",
    "fresh-install": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro* && rm -rf node_modules && rm yarn.lock && yarn cache clean && yarn install",
    "pre-build": "yarn test && yarn lint && yarn flow",
    "postinstall": "yarn flow-update",
    "create-module": "node src/scripts/create-module/index"
  },
  "keywords": [
    "react",
    "native",
    "template",
    "webpack",
    "redux"
  ],
  "author": "Mustafa Arikan",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-expo": "4.0.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.0",
    "eslint-plugin-react": "7.10.0",
    "file-loader": "1.1.11",
    "file-system": "2.2.2",
    "flow-bin": "0.74.0",
    "flow-typed": "2.5.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.3.0",
    "prompt": "1.0.0",
    "react-native-scripts": "1.14.0",
    "webpack": "4.15.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4",
    "yargs": "11.1.0"
  },
  "dependencies": {
    "@material-ui/core": "1.3.1",
    "axios": "0.18.0",
    "expo": "27.1.0",
    "immutable": "^3.8.2",
    "react": "16.3.1",
    "react-dom": "16.4.1",
    "react-native": "~0.55.2",
    "react-native-material-ui": "1.30.1",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-native": "4.3.0",
    "redux": "4.0.0",
    "redux-actions": "2.4.0",
    "redux-form": "7.4.2",
    "redux-immutablejs": "0.0.8",
    "redux-thunk": "2.3.0",
    "styled-components": "3.3.3"
  },
  "jest": {
    "setupFiles": [
      "./test/enzyme-setup.js"
    ],
    "preset": "react-native",
    "moduleDirectories": [
      "node_modules",
      "src/app"
    ]
  }
}
